
# Generated by FastPlaz

# CORS Enabling
Header set Access-Control-Allow-Origin "*"
Header set Access-Control-Allow-Methods "GET, POST, PUT, DELETE, OPTIONS"

RewriteEngine On
AddHandler cgi-script .cgi
AddHandler cgi-script .bin
AddHandler cgi-script .pas
AddHandler cgi-script .exe
Options +ExecCGI

DirectoryIndex your_binary_file index.html
ErrorDocument 500 500.json
<Files "500.json">
   ForceType application/json
</Files>

# custom 404 error
#ErrorDocument 404 "Request: %{THE_REQUEST} \nReferrer: %{HTTP_REFERER} \nHost: %{HTTP_HOST}"
#RewriteRule ^ - [L,R=404]

#environment
SetEnvIf Authorization "(.*)" Authorization=$1

# other bin template
#RewriteRule ^(auth)/(.*)$ auth.bin/$2 [QSA,L]

# main
RewriteCond %{REQUEST_FILENAME} -d [OR]
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -l
RewriteRule ^(.*)$ - [NC,L]
RewriteRule ^(.*)$ your_binary_file/$1 [QSA,L]

